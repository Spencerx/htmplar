    "eslint-config-adidas-babel": "^2.0.0",
    "eslint-config-adidas-env": "^2.1.0",
    "eslint-config-adidas-es8": "^2.0.0",
    "eslint-config-adidas-react": "^2.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
